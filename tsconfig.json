{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitReturns": true,
      "noImplicitAny": false,
      "strict": true,
      "strictNullChecks": false,
      "strictFunctionTypes": false,
      "noImplicitThis": false,
      "alwaysStrict": false,
      "outDir": "dist",
      "sourceMap": true,
      "noUnusedLocals": false,
      "target": "ES2022",
      "resolveJsonModule": true
    },
    "compileOnSave": true,
    "include": [
      "src/**/*"
    ]
  }